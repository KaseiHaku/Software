{
  /******* 数据流向简介 *******
   * data 从 inbound 进入 v2ray 
   * -> v2ray 根据数据特征，匹配 routing 
   * -> 根据匹配到的 routing，v2ray 将 data 转发到 outbound 进行发送
   * */
  
  
  
  /******* 主要配置 *******/
  /* 入站连接配置 */
  "inbounds": [],
  /* 路由配置 */
  "routing": {},
  /* 出站连接配置 */
  "outbounds": [],
  
  
  
  
  /******* 其他配置 *******/
  /* 日志配置 */
  "log": {},
  /* 内置的远程控置 API */
  "api": {},
  /* 内置的 DNS 服务器，若此项不存在，则默认使用本机的 DNS 设置 */
  "dns": {},
  /* 当此项存在时，开启统计信息 */
  "stats": {},
  
  /* 日志配置 */
  "policy": {},
  /* 反向代理配置 */
  "reverse": {},
  
  
  /* 用于配置 V2Ray 如何与其它服务器建立和使用网络连接 */
  "transport": {}
}
